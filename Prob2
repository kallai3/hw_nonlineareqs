clear all
m = input('Initial guess: [x y z]')
dx = 1
i = 0
while (abs(dx) > (.001))
    i=i+1
    f = [(10*(m(2)-m(1))) 28*m(1)-m(2)-m(1)*m(3) m(1)*m(2)-8*m(3)/3]
    J = [-10 28-m(3) m(2);
        10 -1 m(1);
        0 -m(1) -8/3];
    dx =f/J
    m= m - dx
    xplot(i)=m(1)
    yplot(i)=m(2)
    zplot(i)=m(3)
    
end
hold on
pn = input('plot number?')
subplot(2, 2 ,pn)
plot3(xplot, yplot, zplot, 'r-', m(1), m(2), m(3))
